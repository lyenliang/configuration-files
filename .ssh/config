# Set default ssh private key
IdentityFile ~/.ssh/my_private_key.pem

# Prevent ssh timeout
ServerAliveInterval 120

# You can connect to server with "ssh myRemoteMachine" after this config is set
# "mrm" is the alias of myRemoteMachine
# "ssh mrm" is also available
Host myRemoteMachine mrm
HostName 10.50.10.152
User ubuntu
Port 22

